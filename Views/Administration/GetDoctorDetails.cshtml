@model IEnumerable<Diagnostic_Medical_Center.Models.Doctor>
@{
    Layout = null;
 }
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetAgentDetails</title>
    <link href="https://fonts.googleapis.com/css2?family=Muli:wght@200;300;400;500;800&display=swap" rel="stylesheet">
</head>
<body>
    <div>
        <table class="table table-striped table-condensed">
            <tbody>
                <tr>
                    <th>Doctor Id</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Age</th>
                    <th>Gender</th>
                    <th>Contact Number</th>
                    <th>Status</th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.DoctorId</td>
                        <td>@item.FirstName</td>
                        <td>@item.LastName</td>
                        <td>@item.Age</td>
                        <td>@item.Sex</td>
                        <td>@item.PhoneNo</td>
                        @if (!item.RegistrationStatus)
                        {
                            <td>Pending</td>
                        }
                        else if (item.RegistrationStatus)
                        {
                            <td>Accepted</td>
                        }
                        <td>@Html.ActionLink("Accept", "AcceptDoctor", "Administration", new { @id = item.DoctorId }, null)</td>
                        <td>
                            @Html.ActionLink("Reject", "RejectDoctor", "Administration", new { @id = item.DoctorId }, null)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>

